#include<iostream>

using namespace std;

int a[101];

int main()
{
    string s;
    int x;
    cin >> s;
    for(int i=0;i<s.size();i++)
    {
        a[s[i]]++;
        if(a[s[i]]==1){
            x++;
        }
    }
    if(x%2==0){
        cout << "CHAT WITH HER!";
    }
    else{
        cout << "IGNORE HIM!";
    }
}
